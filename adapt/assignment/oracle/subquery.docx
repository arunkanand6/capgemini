1.	SELECT F.ACCOUNT_NO, C.CUST_FNAME, C.CUST_LNAME
FROM BANK_FD_ACCOUNT F
JOIN BANK_CUSTOMER C
USING (CUST_ID)
WHERE F.INITIAL_AMT= (SELECT MAX(INITIAL_AMT) FROM BANK_FD_ACCOUNT) 
    		OR F.INITIAL_AMT = (SELECT MIN(INITIAL_AMT) FROM BANK_FD_ACCOUNT);

2.	SELECT *
FROM BANK_TRANSACTION T
JOIN BANK_RD_ACCOUNT R
ON T.TRANS_ACC_NO=R.ACCOUNT_NO
WHERE R.RD_CUST_ID IN 
    		(SELECT CUST_ID 
    		FROM BANK_CUST_CONTACT 
    		WHERE CUST_STATE = 'Tamilnadu');

3.	SELECT *
FROM BANK_EMPLOYEE
WHERE BANK_EMP_ID NOT IN
		(SELECT BANK_EMP_ID
		FROM BANK_EMPLOYEE
		WHERE DESIGNATION = 'ATTENDER'
		UNION
		SELECT BANK_EMP_ID
		FROM BANK_TRANSACTION
		JOIN BANK_EMPLOYEE
		USING (BANK_EMP_ID));


4.	SELECT CUST_ID, CUST_FNAME
FROM BANK_CUSTOMER
WHERE CUST_TYPE <> 'IND' AND
CUST_ID IN
		(SELECT CUST_ID
		FROM BANK_CUST_CONTACT
		WHERE CONTACT_TYPE <> 'HOME');

5.	SELECT S.ACCOUNT_NO, T.TRANS_AMT
FROM BANK_TRANSACTION T
JOIN BANK_SB_ACCOUNT S
ON S.ACCOUNT_NO=T.TRANS_ACC_NO
WHERE T.TRANS_AMT >
		(SELECT AVG(TRANS_AMT)
		FROM BANK_TRANSACTION);

6.	SELECT ACCOUNT_NO
FROM BANK_SB_ACCOUNT
WHERE NOT EXISTS
		(SELECT *
		FROM BANK_TRANSACTION T
    		WHERE BANK_SB_ACCOUNT.ACCOUNT_NO=T.TRANS_ACC_NO);
